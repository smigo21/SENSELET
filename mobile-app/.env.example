# EATMS Mobile App Environment Variables

# API Configuration
REACT_APP_API_URL=http://localhost:8000/api
REACT_APP_WS_URL=ws://localhost:8000

# Authentication
REACT_APP_JWT_SECRET=your-jwt-secret-here

# SMS Gateway (for USSD integration)
REACT_APP_SMS_GATEWAY_URL=https://api.sms-gateway.com
REACT_APP_SMS_API_KEY=your-sms-api-key

# Map Services
REACT_APP_MAP_API_KEY=your-map-api-key

# File Upload
REACT_APP_MAX_FILE_SIZE=10485760
REACT_APP_ALLOWED_FILE_TYPES=jpg,jpeg,png,pdf

# Feature Flags
REACT_APP_ENABLE_OFFLINE_MODE=true
REACT_APP_ENABLE_PUSH_NOTIFICATIONS=true
REACT_APP_ENABLE_LOCATION_TRACKING=true

# Development
REACT_APP_DEBUG=true
REACT_APP_LOG_LEVEL=debug

# Analytics
REACT_APP_ANALYTICS_ID=your-analytics-id

# Payment Gateway
REACT_APP_PAYMENT_GATEWAY_URL=https://api.payment-gateway.com
REACT_APP_PAYMENT_API_KEY=your-payment-api-key
